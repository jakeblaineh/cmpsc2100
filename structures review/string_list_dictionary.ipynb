   "source": [
    "def histogram(string_list):\n",
    "    hist = dict()\n",
    "    for string in string_list:\n",
    "        if string not in hist:\n",
    "            hist[string] = 0\n",
    "        hist[string] = hist[string] + 1\n",
    "    \n",
    "    return hist\n",
    "\n",
    "histogram(['word', 'text', 'word', 'word', 'string'])\n"
   ]
